{
  "name": "algebraicworkflows",
  "description": "AlgebraicWorkflows -  A graphical editor for automation workflows in VS Code",
  "version": "0.0.1",
  "publisher": "mehalter",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mehalter/algebraicworkflows"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onWebviewPanel:algebraicworkflows"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "algebraicworkflows.editor",
        "title": "AlgebraicWorkflow Editor",
        "category": "AlgebraicWorkflows"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -w -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^16.18.34",
    "@types/vscode": "^1.73.0",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vscode/vsce": "^2.24.0",
    "eslint": "^8.26.0",
    "typescript": "^5.3.2"
  }
}
